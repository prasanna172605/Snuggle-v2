# IMPORTANT: Never commit this file to Git!
# This is a template. Copy to .env.local for development

# Environment
VITE_APP_ENV=development

# Firebase Configuration (Development/Emulator)
VITE_FIREBASE_API_KEY=your-dev-api-key
VITE_FIREBASE_AUTH_DOMAIN=localhost
VITE_FIREBASE_PROJECT_ID=snuggle-dev
VITE_FIREBASE_STORAGE_BUCKET=snuggle-dev.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your-sender-id
VITE_FIREBASE_APP_ID=your-app-id
VITE_FIREBASE_MEASUREMENT_ID=

# Firebase Emulator Ports (for local development)
VITE_FIREBASE_EMULATOR_AUTH_PORT=9099
VITE_FIREBASE_EMULATOR_DATABASE_PORT=9000
VITE_FIREBASE_EMULATOR_STORAGE_PORT=9199
VITE_FIREBASE_EMULATOR_FUNCTIONS_PORT=5001

# Use Firebase Emulators
VITE_USE_EMULATORS=true

# Feature Flags (Development - all enabled)
VITE_FEATURE_DARK_MODE=true
VITE_FEATURE_VIDEO_CALLS=true
VITE_FEATURE_STORIES=true
VITE_FEATURE_CIRCLES=false

# Analytics
VITE_ANALYTICS_ENABLED=false

# API Configuration
VITE_API_BASE_URL=http://localhost:5001

# Logging
VITE_LOG_LEVEL=debug
